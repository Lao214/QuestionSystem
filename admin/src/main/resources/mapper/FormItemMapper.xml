<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.adminService.mapper.FormItemMapper">


    <update id = "updateByFormId" parameterType = "com.example.adminService.entity.FormItem">
        UPDATE form_item SET
        item = #{item}
        WHERE form_id = #{formId};
    </update>

</mapper>
